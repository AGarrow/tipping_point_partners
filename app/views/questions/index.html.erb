<% provide :title,"FAQ's" %>
<div class = "container">
	<div class = "row">
		<h1>Frequently Asked Questions
		<%= link_to raw('<i class= "icon-plus"></i>'), new_question_path, :class => 'btn btn-success pull-right' if can? :create, :questions%>
		</h1>
	</div>
	<div class = "well">
		<% @questions.each do |question| %>
			<%= render :partial =>'show', :locals => {:question => question} %>
		<% end %>
	</div>
</div>